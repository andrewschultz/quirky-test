         : goto "gravel_trail" ;
         : return ;
      }
      : print {( "Going out is ambiguous here, as you " + (is_at "west_tiles" || is_at "east_branch" ? "still " : "") + (!has_visited "plain_base" ? "can't really see a good direction yet." : "have more than one direction worth exploring.") )} ;
      : done ;
   }

